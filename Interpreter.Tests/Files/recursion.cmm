number factorial(n) {
    if (n < 0) {
        print("Факториал определён только для неотрицательных чисел.\n")
        return 0
    }
    if (n == 0 || n == 1) {
        return 1
    }
    return n * factorial(n - 1)
}

number main() {
    number num = 10

    number fact = factorial(num)
    if (num >= 0) {
        printf("Факториал числа {} равен {}", num, fact)
    }
}